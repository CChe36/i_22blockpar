FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];
// calculated from https://www.cfd-online.com/Tools/turbulence.php
internalField   uniform .005; // 

boundaryField
{
    wall1
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity	0.05;
	value           $internalField;
    }

    "(inlet1|outlet1|outlet2|top)"
    {
        type            zeroGradient;
    }

    bottom
    {
        type            symmetryPlane;
    }

    defaultFaces
    {
        type            empty;
    }
}


